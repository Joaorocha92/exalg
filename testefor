public class testefor {
    public static void main(String[] args) {
        Scanner leia = new Scanner (System.in);
        
        int x, soma = 0;
        for(int i=0; i<10; i++){
            System.out.println("Digite o item: "+i);
        x = leia.nextInt();
        soma = soma + x;
        }
        System.out.println("Total: "+soma);
    }
  
}
